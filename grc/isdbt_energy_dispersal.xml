<?xml version="1.0"?>
<block>
  <name>Energy Dispersal</name>
  <key>isdbt_energy_dispersal</key>
  <category>ISDBT</category>
  <import>import isdbt</import>
  <make>isdbt.energy_dispersal($tsp_per_frame)</make>

  <param>
    <name>TSP per OFDM Frame</name>
    <key>tsp_per_frame</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
    <vlen>204</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
    <vlen>204</vlen>
  </source>
</block>
